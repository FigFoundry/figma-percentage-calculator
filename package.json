{
  "name": "figma-percentage-calculator",
  "version": "1.0.0",
  "description": "A percentage calculator that computes various values including percentages, rates, and base numbers.",
  "type": "module",
  "main": "dist/canvas.js",
  "scripts": {
    "build": "pnpm build:canvas && pnpm build:plugin",
    "build:canvas": "vite build -c ./vite.config.canvas.ts",
    "build:plugin": "vite build -c ./vite.config.plugin.ts",
    "dev": "concurrently \"pnpm build:canvas --watch\" \"pnpm build:plugin --watch\"",
    "lint": "pnpm lint:js",
    "lint:js": "eslint --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planetabhi/figma-percentage-calculator.git"
  },
  "keywords": [
    "figma",
    "plugin",
    "percentage",
    "calculator",
    "vite",
    "scss"
  ],
  "author": "@planetabhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/planetabhi/figma-percentage-calculator/issues"
  },
  "homepage": "https://github.com/planetabhi/figma-percentage-calculator#readme",
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "vite": "6.2.6"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@figma/plugin-typings": "1.109.0",
    "@new-ui/reset": "^0.0.8",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/node": "22.14.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/eslint-plugin": "8.30.0",
    "@typescript-eslint/parser": "8.30.0",
    "@vitejs/plugin-react": "4.3.4",
    "concurrently": "9.1.2",
    "esbuild": "^0.25.2",
    "eslint": "9.24.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "sass-embedded": "^1.86.3",
    "typescript": "5.8.3",
    "vite-plugin-generate-file": "0.2.0",
    "vite-plugin-singlefile": "2.2.0"
  }
}